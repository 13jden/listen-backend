<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.wx.mapper.UsertestMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.wx.pojo.Usertest">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="test_file_path" property="testFilePath" />
        <result column="avg_score" property="avgScore" />
        <result column="test_time" property="testTime" />
        <result column="endtime" property="endTime" />
        <result column="num" property="num" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, test_file_path, avg_score, test_time, num,end_time
    </sql>

</mapper>
