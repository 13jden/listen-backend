<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.wx.mapper.AudioMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.wx.pojo.Audio">
        <id column="id" property="id" />
        <result column="content" property="content" />
        <result column="path" property="path" />
        <result column="admin_id" property="adminId" />
        <result column="upload_time" property="uploadTime" />
        <result column="test_number" property="testNumber" />
        <result column="avg" property="avg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, content, path, admin_id, upload_time,test_number,avg
    </sql>

</mapper>
